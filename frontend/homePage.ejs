        <div class="limiter center">
            <div class="container-table100">
                <div class="wrap-table100">
                    <div class="table100">
                        <table>
                            <thead>
                                <tr class="table100-head">
                                    <% columns.forEach((column) => { %>
                                        <th class="table100-head"> <%= column %> </th>
                                    <% }) %>
                                </tr>
                            </thead>
                            <tbody>
                                <% rows.forEach((row) => { %>
                                    <tr>
                                        <% row.forEach((data) => { %>
                                            <td> <%= data %> </td>
                                        <% }) %>
                                    </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>



          <div class="center">
            <a href="../flashes.csv" download="keyled_flashes.csv">
                <button class="button"> Download as CSV</button>
            </a>
         </div> 
    </body>
