<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Keyled Statistics Tracker</title>
        <meta name="description" content="">
        <meta http-equiv="refresh" content="30">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="style.css">
    </head>
    <body>
        <div class="header">
            <h1> Keyled Statistics Tracker </h1>
        </div>

        <br>
        <br>

        <table class="center">
            <thead>
            <tr>
                <% columns.forEach((coulmn) => { %>
                    <th> <%= coulmn %> </th>
                <% }) %>
            </tr>
            </thead>

            <tbody>
                <% rows.forEach((row) => { %>
                    <tr>
                        <% row.forEach((data) => { %>
                            <td> <%= data %> </td>
                        <% }) %>
                    </tr>
                <% }) %>
            </tbody>


          </table>
    </body>
</html>